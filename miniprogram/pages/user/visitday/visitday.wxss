/* pages/user/visitday/visitday.wxss */
.visit-container{
  width: 100%;
  height: auto;
}
.visit-container .dates-item{
  width: 100%;
  height: 60px;
  border-bottom:solid 1px #ddd; 
  background: #FFF;
}
.visit-container .user-photo{
  display: inline-block;
  width: 46px;
  height: 46px;
  border-radius: 60%;
  border: solid 1px #ddd;
  margin-left: 20px;
  margin-top: 7px;
}
.visit-container .nick-name{
  position: absolute;
  display: inline-block;
  width: 33%;
  height: 60px;
  line-height: 60px;
  color: #333;
  left: 30%;
  text-align: center;
}
.visit-container .visit-dates{
  position: absolute;
  display: inline-block;
  width: 35%;
  height: 60px;
  line-height: 60px;
  color: #333;
  right: 0;
  text-align: center;
}